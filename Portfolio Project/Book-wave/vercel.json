{
    "version": 2,
    "builds": [
      {
        "src": "src/**/*",
        "use": "@vercel/node"
      },
      {
        "src": "dist/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/assets/(.*)", "dest": "/dist/assets/$1" },
      { "src": "/(.*)", "dest": "/dist/$1" }
    ]
  }
  
   